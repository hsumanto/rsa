<?xml version='1.0' encoding='UTF-8'?>
<query xmlns='http://www.vpac.org/namespaces/rsaquery-0.2'>
	<input id='ds' href='rsa:vic/25m' />
	<input id='boundaries' href='rsa:boundaries/100m' />
	<filter id='LGA' cls='org.vpac.ndg.query.stats.Categories'>
		<literal name='buckets' value='logRegular' />
		<sampler name='input' ref='#ds/Band1' />
		<sampler name='categories' ref='#boundaries/LGA' />
	</filter>
 	<filter id='SA3' cls='org.vpac.ndg.query.stats.Categories'>
		<literal name='buckets' value='logRegular' />
		<sampler name='input' ref='#LGA/output' />
		<sampler name='categories' ref='#boundaries/SA3' />
	</filter>
	<output id='outfile'>
		<grid ref='#ds' />
		<variable name='nothing' ref='#SA3/output' />
	</output>
</query>